        <li id="Organizations">
          <%= button_to "All Organizations", organizations_path, class:"Organizations", type:"button", class:"btn btn-outline-light text-dark", :method => :get %></li>
          <% if user_org_signed_in? %>
            <%= link_to "Logout", destroy_user_org_session_path,method: :delete %>
          <% else %>
        <li id="Login">
          <%= button_to "Login", new_user_org_session_path, class:"login", type:"button", class:"btn btn-outline-light text-dark" %>
          <% end %>
        </li>
      </ul>
    <div class="tech-slideshow">
      <div class="mover-1"></div>
      <div class="mover-2"></div>
    </div>
  </div>
</body>
